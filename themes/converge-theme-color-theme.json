  "tokenColors": [
    {
			"name": "[HTML] - element",
      "scope": "source.js meta.jsx.children.js",
			"settings": {
				"foreground": "#b0bffc"
			}
    },
    {
			"name": "[HTML] - end tag",
      "scope": ["source.js entity.name.tag.js",
        "source.js punctuation.definition.tag.end.js"],
			"settings": {
				"foreground": "#3a54c7"
			}
    },
    {
			"name": "[HTML] - tag",
      "scope": "source.js meta.tag.attributes.js",
			"settings": {
				"foreground": "#6e88fa"
			}
    },
    {
			"name": "[JAVASCRIPT/REACT] - class def",
      "scope": ["source.js storage.type.class.js",
        "source.python storage.type.class.python"],
			"settings": {
				"foreground": "#6e88fa"
			}
    },
    {
			"name": "[JAVASCRIPT/REACT] - state",
      "scope": "source.js variable.object.property.js",
			"settings": {
				"foreground": "#6e88fa"
			}
    },
    {
			"name": "[JAVASCRIPT] - Parameter",
      "scope": "source.js meta.object-literal.key.js",
			"settings": {
				"foreground": "#3051e4"
			}
    },
    {
			"name": "[JAVASCRIPT] - Parameter",
      "scope": ["source.js variable.other.object.property.js",
        "source.python meta.function-call.arguments.python"],
			"settings": {
				"foreground": "#059b11"
			}
    },
    {
			"name": "[JAVASCRIPT] - Parameter",
      "scope": ["source.js string.quoted.single.js",
        "source.python string.quoted.single.python"],
			"settings": {
				"foreground": "#a26ae7"
			}
    },
    {
			"name": "[Python] - Self",
      "scope": ["source.js variable.parameter.js",
        "source.python variable.parameter.function.language.special.self.python",
        "source.python variable.language.special.self.python"],
			"settings": {
				"foreground": "#4069fc"
			}
    },
    {
			"name": "[JAVASCRIPT] - Parameter",
      "scope": ["source.js variable.parameter.js",
        "source.python variable.parameter.function.language.python"],
			"settings": {
				"foreground": "#1141ec"
			}
    },
    {
			"name": "[JAVASCRIPT] - Decimal",
      "scope": "source.js constant.numeric.decimal.js",
			"settings": {
				"foreground": "#fbbc62"
			}
    },
    {
			"name": "[JAVASCRIPT] - Null",
      "scope": "source.js constant.language.null.js",
			"settings": {
				"foreground": "#fbbc62"
			}
    },
    {
			"name": "[JAVASCRIPT] - boolean",
      "scope": ["source.js constant.language.boolean.false.js",
               "source.js constant.language.boolean.true.js"],
			"settings": {
				"foreground": "#d5b3d6"
			}
    },
    {
			"name": "[JAVASCRIPT] - Comment double slash",
      "scope": ["source.js comment.line.double-slash.js",
        "source.python string.quoted.docstring.multi.python"],
			"settings": {
				"foreground": "#bbbbbb"
			}
    },
    {
			"name": "[JAVASCRIPT] - Comment start",
			"scope": "source.js punctuation.definition.comment.js",
			"settings": {
				"foreground": "#bbbbbb"
			}
    },
    {
			"name": "[JAVASCRIPT] - Other Property",
			"scope": "source.js variable.other.property.js",
			"settings": {
				"foreground": "#01a19c"
			}
    },
    {
			"name": "[JAVASCRIPT] - Control conditional",
			"scope": "source.js keyword.control.conditional.js",
			"settings": {
				"foreground": "#00f1e8"
			}
    },
    {
			"name": "[JAVASCRIPT] - Read write",
			"scope": "source.js variable.other.readwrite.js",
			"settings": {
				"foreground": "#fb6c78"
			}
    },
    {
			"name": "[JAVASCRIPT] - Arrow Function",
			"scope": "source.js storage.type.function.arrow.js",
			"settings": {
				"foreground": "#9367fa"
			}
    },
    {
			"name": "[JAVASCRIPT] - Function name",
      "scope": ["source.js entity.name.function.js",
        "source.python support.function.magic.python",
        "source.python entity.name.function.python",
        "source.python meta.function-call.generic.python"],
			"settings": {
				"foreground": "#db0fe2"
			}
    },
    {
			"name": "[JAVASCRIPT] - Function call",
			"scope": "source.js meta.function-call.js",
			"settings": {
				"foreground": "#d5b3d6"
			}
    },
    {
			"name": "[JAVASCRIPT] - Color for Storage Type Function",
			"scope": "source.js storage.type.function",
			"settings": {
				"foreground": "#1f0520"
			}
    },
    {
			"name": "[JAVASCRIPT] - Color for Variable Language",
      "scope": ["variable.language, entity.name.type.class.js",
        "source.python entity.name.type.class.python"],
			"settings": {
				"foreground": "#1de42d"
			}
    },
    {
			"name": "[JAVASCRIPT] - Color for Inherited Component",
			"scope": ["entity.other.inherited-class"],
			"settings": {
				"foreground": "#FFEE80"
			}
		},
		{
			"name": "[JAVASCRIPT] - Color for React Extends keyword",
			"scope": ["storage.type.extends.js", "storage.type.class.jsdoc"],
			"settings": {
				"foreground": "#FF9D00"
			}
		},
		{
			"name": "[JAVASCRIPT] — Docs Keywords",
			"scope": "punctuation.definition.block.tag.jsdoc",
			"settings": {
				"foreground": "#FF9D00"
			}
		},
		{
			"name": "[JAVASCRIPT] — Doc Var Types",
			"scope": ["variable.other.jsdoc", "entity.name.type.instance.jsdoc"],
			"settings": {
				"foreground": "#9EFFFF"
			}
		},
		{
			"name": "[JAVASCRIPT] — Var Types",
			"scope": ["variable.other.constant.js"],
			"settings": {
				"foreground": "#fb6c78"
			}
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage.type.js",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#c43c85"
      }
    },
    {
      "name": "[JS] EXPORT",
      "scope": [
        "keyword.control.export.js"
      ],
      "settings": {
        "foreground": "#6822b7"
      }
    },
    {
      "name": "[JS] STRING",
      "scope": [
        "string.quoted.double.js"
      ],
      "settings": {
        "foreground": "#a26ae7"
      }
    },
    {
      "name": "[JS] FUNCTION CALL",
      "scope": [
        "variable.other.object.js"
      ],
      "settings": {
        "foreground": "#01a19c"
      }
    }
  ]
}